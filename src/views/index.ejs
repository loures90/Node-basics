<html>
    <head></head>
    <body>
        <div class="container">
            <div class="content">
                <% if( users.length > 0) {  %> <%  users.forEach(user => { %>
                    <h3 class="title"> <%= user.firstName %> <%= user.lastName%>  </h3>
                    <p class="title"> <%= user.email %> </p>
                    <% }) %> <% } else { %>
                        <p> Não há usuários</p>
                    <% } %>

            </div>
        </div>
    </body>
</html>